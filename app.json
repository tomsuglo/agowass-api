{ 
	"expo": {
    	"name": "AGOWASS App",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/schoolLogo.png",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"slug": "AGOWASSapp",
		"splash": {
			"image": "./assets/images/schoolLogo.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"scheme": "agowassapp", 
		"ios": {
			"supportsTablet": true,
			"infoPlist": {
				"ITSAppUsesNonExemptEncryption": false
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/schoolLogo.png",
				"backgroundColor": "#ffffff"
			},
			"permissions": [
				"android.permission.RECORD_AUDIO",
				"android.permission.MODIFY_AUDIO_SETTINGS"
			],
			"package": "xyz.create.CreateExpoEnvironment"
		},
		"plugins": [
			[
				"expo-router",
				{
					"sitemap": false
				}
			],
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/schoolLogo.png",
					"imageWidth": 200,
					"resizeMode": "contain"
				}
			],
			"expo-audio",
			[
				"expo-build-properties",
				{
					"ios": {
						"useFrameworks": "static"
					}
				}
			],
			"expo-video"
		],
		"web": {
			"bundler": "metro",
			"favicon": "./assets/images/favicon.png"
		},
		"experiments": {
			"router": {
				"root": "src/app"
			}
		},
		"extra": {
			"router": {
				"origin": false
			}
		},
		"runtimeVersion": {
			"policy": "appVersion"
		}
	}
}
